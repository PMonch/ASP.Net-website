
@{
    ViewBag.Title = "Index";
}

<div class="ui five cards">
    @foreach (var p in ViewBag.MyList)
    {
        string pr = p.Name.ToString();

        <div class="card">
           
            <div class="image">
                <img src="data:image/jpg;base64,">
            </div>
            <div class="extra">
                @Html.ActionLink(pr, "ProductDetails", "Home", new { name = pr }, null)
            </div>
            <div class="extra">
                <i class="euro icon">@p.Price</i>
           </div>
        </div>
        
    }

</div>



